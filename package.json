{
  "name": "astroclient-launcher",
  "version": "0.6.0",
  "description": "The electron launcher of Astro Client",
  "main": "dist/app.js",
  "scripts": {
    "start": "electron ./dist/app.js",
    "build": "electron-builder build --win --publish never",
    "deploy": "electron-builder build --win --publish always",
    "postinstall": "electron-builder install-app-deps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Astro-Client/Launcher-Public.git"
  },
  "author": "Next Origin, INC.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Astro-Client/Launcher-Public/issues"
  },
  "homepage": "https://github.com/Astro-Client/Launcher-Public#readme",
  "devDependencies": {
    "@types/async": "^3.2.12",
    "@types/discord-rich-presence": "^0.0.0",
    "@types/discord-rpc": "^4.0.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.14",
    "@types/os-utils": "^0.0.1",
    "@types/underscore": "^1.11.4",
    "@types/ws": "^8.2.2",
    "electron": "^17.1.0",
    "electron-builder": "^22.14.13",
    "install": "^0.13.0",
    "npm": "^8.3.2"
  },
  "dependencies": {
    "async": "^3.2.3",
    "axios": "^0.25.0",
    "colors": "^1.4.0",
    "discord-rich-presence": "0.0.8",
    "draftlog": "^1.0.13",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^4.6.5",
    "folder-hash": "^4.0.1",
    "fs": "0.0.1-security",
    "jquery": "^3.5.1",
    "jsonwebtoken": "^8.5.1",
    "minecraft-launcher-core": "^3.16.11",
    "nodejs-file-downloader": "^4.7.1",
    "nodejs-websocket": "^1.7.2",
    "os-utils": "0.0.14",
    "request": "^2.88.2",
    "underscore": "^1.13.2"
  },
  "build": {
    "appId": "net.astroclient.net",
    "directories": {
      "output": "build"
    },
    "files": [
      "**/*",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}",
      "!**/src/*.d.ts",
      "!**/src/*.ts",
      "!**/start_Astro_Client.lnk",
      "!**/tsconfig.json",
      "!**/creditlogin",
      "!**/electron-builder.yml"
     ],
     "copyright": "Copyright Â© 2022 Astro Client & Next Origin",
     "productName": "AstroClient"
  }
}